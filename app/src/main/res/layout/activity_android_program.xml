<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/fill_fill"
    android:orientation="vertical"
    android:background="#ededed">

    <com.qk.applibrary.widget.TopbarView
        android:id="@+id/top_bar_view"
        style="@style/wrap_wrap" />

    <RelativeLayout
        android:id="@+id/abnormal_check_out"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_50_dip"
        android:layout_marginLeft="@dimen/dimen_13_dip"
        android:layout_marginRight="@dimen/dimen_13_dip"
        android:layout_marginTop="@dimen/dimen_15_dip"
        android:background="@drawable/mybelt_bg">

        <ImageView
            android:id="@+id/room_iv"
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:src="@drawable/qq_icon" />

        <TextView
            android:textColor="#333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:layout_toRightOf="@+id/room_iv"
            android:gravity="center_vertical"
            android:text="360手机卫士"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_8_dip"
            android:src="@drawable/arrow_right" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/normal_check_out"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_50_dip"
        android:layout_marginLeft="@dimen/dimen_13_dip"
        android:layout_marginRight="@dimen/dimen_13_dip"
        android:layout_marginTop="@dimen/dimen_15_dip"
        android:background="@drawable/mybelt_bg">

        <ImageView
            android:id="@+id/erery_day_reconnoitore_list_iv"
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:src="@drawable/qq_icon" />

        <TextView
            android:textColor="#333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:layout_toRightOf="@+id/erery_day_reconnoitore_list_iv"
            android:gravity="center_vertical"
            android:text="智慧上海"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_8_dip"
            android:src="@drawable/arrow_right" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/no_settle_check_out"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_50_dip"
        android:layout_marginLeft="@dimen/dimen_13_dip"
        android:layout_marginRight="@dimen/dimen_13_dip"
        android:layout_marginTop="@dimen/dimen_15_dip"
        android:background="@drawable/mybelt_bg">

        <ImageView
            android:id="@+id/belt_iv"
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:src="@drawable/qq_icon" />

        <TextView
            android:textColor="#333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:layout_toRightOf="@+id/belt_iv"
            android:gravity="center_vertical"
            android:text="谷歌市场"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_8_dip"
            android:src="@drawable/arrow_right" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/statement_search"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_50_dip"
        android:layout_marginLeft="@dimen/dimen_13_dip"
        android:layout_marginRight="@dimen/dimen_13_dip"
        android:layout_marginTop="@dimen/dimen_15_dip"
        android:background="@drawable/mybelt_bg">

        <ImageView
            android:id="@+id/inverstigation_iv"
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:src="@drawable/qq_icon" />

        <TextView
            android:textColor="#333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_19_dip"
            android:layout_toRightOf="@+id/inverstigation_iv"
            android:gravity="center_vertical"
            android:text="开源中国"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="@dimen/dimen_20_dip"
            android:layout_height="@dimen/dimen_20_dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_8_dip"
            android:src="@drawable/arrow_right" />
    </RelativeLayout>


</LinearLayout>
