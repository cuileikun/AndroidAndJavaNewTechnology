<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/dimen_5_dip"
    android:layout_marginRight="@dimen/dimen_5_dip"
    android:background="#ffffff"
    android:orientation="vertical">

    <TextView
        android:id="@+id/coming_soon_tv_room_address"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_30_dip"
        android:layout_marginLeft="@dimen/dimen_5_dip"
        android:layout_marginRight="@dimen/dimen_5_dip"
        android:layout_marginTop="@dimen/dimen_10_dip"
        android:background="@drawable/servicecenter_abnormal_checkout_item_bg"
        android:gravity="center"
        android:text="1南宁路501弄27号201室E室"
        android:textColor="#ffffff"
        android:textSize="14sp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_5_dip"
        android:layout_marginRight="@dimen/dimen_5_dip"
        android:background="@drawable/fragment_item_bg"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/first_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/coming_soon_tv_city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_marginRight="@dimen/dimen_5_dip"
                android:layout_marginTop="@dimen/dimen_15_dip"
                android:gravity="center_vertical"
                android:text="城市：上海"
                android:textColor="#333333"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/coming_soon_tv_company"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/coming_soon_tv_city"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_marginRight="@dimen/dimen_5_dip"
                android:layout_marginTop="@dimen/dimen_15_dip"
                android:text="分公司：徐汇分公司"
                android:textColor="#333333"
                android:textSize="14sp" />

        </RelativeLayout>

        <TextView
            android:id="@+id/coming_soon_tv_service_center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/first_rl"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="服务中心：第三服务中心"
            android:textColor="#333333"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/coming_soon_tv_fgy_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_service_center"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="房管员：张三"
            android:textColor="#333333"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/coming_soon_tv_company_price"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_fgy_name"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="公司定价:6000元/月"
            android:textColor="#333333"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/coming_soon_tv_sale_price"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_company_price"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="销售价格:6000月/月"
            android:textColor="#333333"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/coming_soon_tv_sign_pirce"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_sale_price"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="签单价格:6000元/月"
            android:textColor="#333333"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/coming_soon_tv_grace_period"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_sign_pirce"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="优惠期：2016-12-30--2018-12-30"
            android:textColor="#333333"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/coming_soon_tv_contract_period"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_grace_period"
            android:layout_marginLeft="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_5_dip"
            android:layout_marginTop="@dimen/dimen_15_dip"
            android:text="合同期：2016-12-30--2018-12-30"
            android:textColor="#333333"
            android:textSize="14sp" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/coming_soon_tv_contract_period"
            android:layout_marginBottom="@dimen/dimen_15_dip"
            android:layout_marginRight="@dimen/dimen_18_dip"
            android:layout_marginTop="@dimen/dimen_15_dip">

            <TextView
                android:id="@+id/coming_soon_tv_net_profit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="【net profit:337.6元】"
                android:textColor="#666666"
                android:textSize="12sp" />
        </RelativeLayout>




    </RelativeLayout>

</LinearLayout>
